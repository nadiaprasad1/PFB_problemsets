#!/usr/bin/env python3
dna = ''
dna_count = dna.upper
A = dna.count('A')
print('the number of A is:',A)
T = dna.count('T')
print('the number of T is:',T)
G = dna.count('G')
print('the number of G is:',G)
C = dna.count('C')
print('the number of C is:',C)

#number 7
dna_test = 'ATATATA'
rna_test = dna_test.replace('T','U')
print('DNA to RNA test is:', dna_test,'turns into',rna_test)

dna = ''

rna = dna.replace('T','U')
print('DNA to RNA is',dna,'turns into',rna)

dna_mixedcase = ''
dna_upperonly = dna_mixedcase.upper
rna_upperonly=dna.replace('T','U')

A_count=dna_upperonly.count('A')
T_count=dna_upperonly.count('T')
AT_count=A_count + T_count

